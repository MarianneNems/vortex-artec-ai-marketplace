{
  "name": "vortex-ai-marketplace",
  "version": "3.0.0",
  "description": "Frontend assets and build system for VORTEX AI Marketplace WordPress plugin",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "dev": "webpack --mode=development --watch",
    "serve": "webpack serve --mode=development --open",
    "lint": "eslint public/js/**/*.js tests/js/**/*.js *.js",
    "lint:fix": "eslint public/js/**/*.js tests/js/**/*.js *.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sass:build": "sass public/scss:public/css --style=compressed",
    "sass:watch": "sass public/scss:public/css --watch",
    "clean": "rimraf dist/ public/css/*.css public/js/dist/",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "format": "prettier --write \"**/*.{js,css,scss,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "moment": "^2.29.4",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "lodash": "^4.17.21",
    "bootstrap": "^5.3.2",
    "web3": "^4.3.0",
    "qrcode": "^1.5.3",
    "jszip": "^3.10.1",
    "file-saver": "^2.0.5",
    "sweetalert2": "^11.10.0",
    "tippy.js": "^6.3.7",
    "core-js": "^3.34.0"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "css-loader": "^6.8.1",
    "sass-loader": "^13.3.2",
    "sass": "^1.69.5",
    "mini-css-extract-plugin": "^2.7.6",
    "html-webpack-plugin": "^5.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.55.0",
    "@eslint/js": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-webpack-plugin": "^4.0.1",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.5",
    "lint-staged": "^15.2.0",
    "rimraf": "^5.0.5",
    "cross-env": "^7.0.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "ie >= 11"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/js/setup.js"],
    "testMatch": [
      "<rootDir>/tests/js/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "public/js/**/*.js",
      "admin/js/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["html", "text", "lcov"]
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "prettier --write"],
    "*.{css,scss}": ["prettier --write"],
    "*.json": ["prettier --write"]
  },
  "keywords": [
    "wordpress",
    "plugin",
    "ai",
    "marketplace",
    "blockchain",
    "art",
    "nft",
    "solana",
    "tola"
  ],
  "author": {
    "name": "Mariana Villard",
    "email": "info@vortexartec.com",
    "url": "https://vortexartec.com"
  },
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarianneNems/vortex-artec-ai-marketplace.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 